AAA;AAAA,uBAIA,KAAA,CAAA,MAAA,CAAA,IAAA,CACA,qBADA,EAEA,KAAA,CAAA,EAAA,CAAA,SAAA,CAAA,IAAA,CAFA,CAJA;;AAAA;AAIA,gBAAA,GAJA;;AAQA,oBAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,IAAA,KAAA,SAAA,EAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,UAAA;;AACA,kBAAA,KAAA,CAAA,cAAA,CAAA;AACA,oBAAA,WAAA,EAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,UADA;AAEA,oBAAA,aAAA,EAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AAFA,mBAAA;;AAIA,kBAAA,KAAA,CAAA,OAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA;;AAhBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA;AA1BA,IAlBA;AA8CA,EAAA,KAAA,EAAA;AACA,IAAA,EAAA,EAAA;AACA,MAAA,OADA,mBACA,MADA,EACA;AACA,aAAA,cAAA;AACA,aAAA,YAAA,CAAA,KAAA,EAAA;AACA,OAJA;AAKA,MAAA,IAAA,EAAA,IALA;AAMA,MAAA,SAAA,EAAA;AANA;AADA,GA9CA;AAwDA,EAAA,OAxDA,qBAwDA;AACA;AACA,SAAA,YAAA,CAAA,KAAA,EAAA;AACA;AA3DA,CAAA","sourcesContent":["<template>\n  <div class=\"help\">\n    <div v-title data-title=\"问答详情\"></div>\n\n    <!-- Start of Page Container -->\n    <div class=\"page-container\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <!-- start of page content -->\n          <div class=\"span8 page-content\">\n            <article class=\"type-post format-standard hentry clearfix\">\n              <h1 class=\"post-title\">\n                <a>{{ content.help_title }}</a>\n              </h1>\n\n              <div class=\"post-meta clearfix\">\n                <span class=\"date\">{{ content.createtime | dataFormat }}</span>\n                <span class=\"category\">\n                  <el-popover placement=\"right\" width=\"400\" trigger=\"hover\">\n                    <span>\n                      <li\n                        class=\"comment even thread-odd thread-alt depth-1\"\n                        id=\"li-comment-4\"\n                      >\n                        <article id=\"comment-4\">\n                          <img\n                            :src=\"content.avatar\"\n                            class=\"avatar touxiang avatar-60 photo\"\n                            height=\"60\"\n                            width=\"60\"\n                          />\n\n                          <div class=\"comment-meta\">\n                            <h5 class=\"author\">{{ content.nickname }}</h5>\n\n                            <p class=\"date\" v-if=\"content.realstate == 3\">\n                              认证用户\n                            </p>\n                            <p class=\"date\" v-else>未认证用户</p>\n                          </div>\n                        </article>\n                      </li>\n                      <div class=\"xinxi\">\n                        <p style=\"  color: #000;\">账号：</p>\n                        <p>{{ content.username }}</p>\n                      </div>\n                      <div class=\"xinxi\">\n                        <p style=\"  color: #000;\">邮箱：</p>\n                        <p>{{ content.mail }}</p>\n                      </div>\n                      <div class=\"xinxi\">\n                        <p style=\"  color: #000;\">QQ: </p>\n                        <p>{{ content.qq }}</p>\n                      </div>\n                      <div class=\"xinxi\">\n                        <p style=\"  color: #000;\">个人简介：</p>\n                        <p>{{ content.synopsis }}</p>\n                      </div>\n                      <el-button\n                        @click=\"jubao(content.username)\"\n                        style=\"margin:10px 150px\"\n                        type=\"danger\"\n                        plain\n                        >举报</el-button\n                      >\n                    </span>\n                    <a\n                      href=\"#\"\n                      slot=\"reference\"\n                      title=\"View all posts in Server &amp; Database\"\n                      >{{ content.nickname }}</a\n                    >\n                  </el-popover>\n                </span>\n                <span class=\"comments\">\n                  <a\n                    href=\"#\"\n                    title=\"Comment on Integrating WordPress with Your Website\"\n                    >{{ commentnum }} Comments</a\n                  >\n                </span>\n                <span class=\"like-count\">66</span>\n              </div>\n              <!-- end of post meta -->\n              <blockquote v-html=\"content.help_content\"></blockquote>\n            </article>\n\n            <div class=\"like-btn\">\n              <form id=\"like-it-form\" action=\"#\" method=\"post\">\n                <span class=\"like-it\">66</span>\n                <input type=\"hidden\" name=\"post_id\" value=\"99\" />\n                <input type=\"hidden\" name=\"action\" value=\"like_it\" />\n              </form>\n\n              <span class=\"tags\">\n                <strong>标签</strong>\n                <span\n                  v-for=\"(tag, id) in content.help_tag\"\n                  @click=\"$router.push(`/help/${tag}`)\"\n                  :key=\"id\"\n                  class=\"label\"\n                  >{{ tag }}</span\n                >\n              </span>\n            </div>\n\n            <comment />\n            <!-- end of comments -->\n            <!-- end of page content -->\n          </div>\n          <aside class=\"span4 page-sidebar\">\n            <carousel />\n            <help />\n          </aside>\n\n          <!-- end of sidebar -->\n        </div>\n      </div>\n    </div>\n    <!-- End of Page Container -->\n  </div>\n</template>\n\n<script>\nimport comment from '@/components/comment.vue'\nimport help from '@/components/Q&A.vue'\n\nimport { mapState, mapActions } from 'vuex'\n\nexport default {\n  components: {\n    comment,\n    help\n  },\n  data() {\n    return {\n      content: {}\n    }\n  },\n  props: {\n    id: {}\n  },\n  computed: {\n    ...mapState({\n      commentnum: state => state.commentnum\n    })\n  },\n  methods: {\n    ...mapActions(['setcontentid', 'setcontentinfo']),\n    jubao(username) {\n      let url = escape(window.location.href)\n      this.$router.push({\n        path: '/jubao',\n        query: { user: username, url: url }\n      })\n    },\n    async gethelpcontent() {\n      let data = {\n        id: this.id\n      }\n      let res = await this.$axios.post(\n        '/web/gethelpcontent',\n        this.qs.stringify(data)\n      )\n      if (res.data.state.type === 'SUCCESS') {\n        this.content = res.data.data\n        console.log(res.data.data.help_title)\n        this.setcontentinfo({\n          contentname: res.data.data.help_title,\n          contentuserid: res.data.data.user_id\n        })\n        this.content.help_tag = res.data.data.help_tag.split(',')\n      }\n    }\n  },\n  watch: {\n    id: {\n      handler(newVal) {\n        this.gethelpcontent()\n        this.setcontentid(this.id)\n      },\n      deep: true,\n      immediate: true\n    }\n  },\n  created() {\n    // this.gethelpcontent();\n    this.setcontentid(this.id)\n  }\n}\n</script>\n<style>\n.help {\n  min-height: 200px;\n}\n.label {\n  margin-left: 15px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}