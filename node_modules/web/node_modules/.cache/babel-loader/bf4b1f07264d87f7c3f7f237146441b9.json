AA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IADA,GACA;AAAA,kBAAA,EAAA,EAAA,KAAA,CAAA;AAAA,iBADA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,yBAAA,EAAA,KAAA,CAAA,EAAA,CAAA,SAAA,CAAA,IAAA,CAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;;AAGA,oBAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,IAAA,KAAA,SAAA,EAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;;AACA,kBAAA,KAAA,CAAA,cAAA,CAAA;AACA,oBAAA,WAAA,EAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,cADA;AAEA,oBAAA,aAAA,EAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AAFA,mBAAA;AAIA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAnBA;AAoBA,IAAA,OApBA,qBAoBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IADA,GACA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,MAAA,CAAA,IADA;AAEA,kBAAA,WAAA,EAAA,MAAA,CAAA,OAAA,CAAA,cAFA;AAGA,kBAAA,SAAA,EAAA,MAAA,CAAA,aAHA;AAIA,kBAAA,UAAA,EAAA,MAAA,CAAA,OAAA,CAAA;AAJA,iBADA;AAAA;AAAA,uBAOA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,cAAA,EAAA,MAAA,CAAA,EAAA,CAAA,SAAA,CAAA,IAAA,CAAA,CAPA;;AAAA;AAOA,gBAAA,GAPA;;AAQA,oBAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,IAAA,KAAA,SAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,aAAA;AACA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAjCA;AAkCA,IAAA,mBAlCA,iCAkCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,yBAAA,EAAA,MAAA,CAAA,EAAA,CAAA,SAAA,CAAA;AAAA,kBAAA,UAAA,EAAA,MAAA,CAAA;AAAA,iBAAA,CAAA,CADA;;AAAA;AACA,gBAAA,GADA;;AAEA,oBAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,IAAA,KAAA,SAAA,EAAA;AACA,kBAAA,MAAA,CAAA,gBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA;AAvCA,IAtBA;AA+DA,EAAA,KAAA,EAAA;AACA,IAAA,EAAA,EAAA;AACA,MAAA,OADA,mBACA,MADA,EACA;AACA,aAAA,kBAAA;AACA,aAAA,mBAAA;AACA,aAAA,YAAA,CAAA,KAAA,EAAA;AACA,OALA;AAMA,MAAA,IAAA,EAAA,IANA;AAOA,MAAA,SAAA,EAAA;AAPA;AADA,GA/DA;AA0EA,EAAA,OA1EA,qBA0EA;AACA,SAAA,mBAAA;AACA,SAAA,YAAA,CAAA,KAAA,EAAA;AACA;AA7EA,CAAA","sourcesContent":["<template>\n  <div class=\"activitycontent\">\n    <article class=\"type-post format-standard hentry clearfix\">\n      <h3>{{ content.activity_title }}</h3>\n      <div>\n        <div class=\"show_unit fl ativity\">\n          <a class=\"iconfont ic\">&#xe622;</a>\n          <a class=\"tagname\">活动名称:</a>\n          {{ content.activity_title }}\n        </div>\n        <el-popover placement=\"left\" width=\"400\" trigger=\"hover\">\n          <span>\n            <li class=\"comment even thread-odd thread-alt depth-1\" id=\"li-comment-4\">\n              <article id=\"comment-4\">\n                <img\n                  :src=\"content.avatar\"\n                  class=\"avatar touxiang avatar-60 photo\"\n                  height=\"60\"\n                  width=\"60\"\n                />\n                <div class=\"comment-meta\">\n                  <h5 class=\"author\">{{ content.nickname }}</h5>\n                  <p class=\"date\" v-if=\"content.realstate == 3\">认证用户</p>\n                  <p class=\"date\" v-else>未认证用户</p>\n                </div>\n              </article>\n            </li>\n            <div class=\"xinxi\">\n              <p style=\"color: #000;\">账号：</p>\n              <p>{{ content.username }}</p>\n            </div>\n            <div class=\"xinxi\">\n              <p style=\"color: #000;\">邮箱：</p>\n              <p>{{ content.mail }}</p>\n            </div>\n            <div class=\"xinxi\">\n              <p style=\"color: #000;\">QQ：</p>\n              <p>{{ content.qq }}</p>\n            </div>\n            <div class=\"xinxi\">\n              <p style=\"color: #000;\">个人简介：</p>\n              <p>{{ content.synopsis }}</p>\n            </div>\n            <el-button\n              @click=\"jubao(content.username)\"\n              style=\"margin: 10px 150px\"\n              type=\"danger\"\n              plain\n              >举报</el-button\n            >\n          </span>\n          <div slot=\"reference\" class=\"show_unit fl ativity\">\n            <a class=\"iconfont ic\">&#xe66a;</a>\n            <a class=\"tagname\">发起者:</a>\n            {{ content.nickname }}\n          </div>\n        </el-popover>\n        <div class=\"show_unit fl ativity\">\n          <a class=\"iconfont ic\">&#xe62a;</a>\n          <a class=\"tagname\">活动时间:</a>\n          {{ content.activity_statetime | dataFormat }}——{{ content.activity_endtime | dataFormat }}\n        </div>\n        <div class=\"show_unit fl ativity\">\n          <a class=\"iconfont ic\">&#xe62a;</a>\n          <a class=\"tagname\">活动人数:</a>\n          {{ content.activity_num }}\n        </div>\n        <div class=\"show_unit fl ativity\">\n          <a class=\"iconfont ic\">&#xe62a;</a>\n          <a class=\"tagname\">活动地点:</a>\n          {{ content.activity_locale }}\n        </div>\n        <div style=\"clear:both\"></div>\n      </div>\n      <h3>活动描述</h3>\n      <blockquote v-html=\"content.activity_content\"></blockquote>\n    </article>\n    <el-button\n      type=\"primary\"\n      @click=\"setjoin\"\n      style=\"width: 80px; margin: 0 auto; display: block;\"\n      >参加</el-button\n    >\n    <div class=\"block\">\n      <h3>活动公告</h3>\n      <el-timeline>\n        <el-timeline-item\n          v-for=\"(item, id) in announcementlist\"\n          :key=\"id\"\n          :timestamp=\"item.announcement_createtime | dataFormat\"\n          placement=\"top\"\n        >\n          <el-card>\n            <h4>{{ item.announcement_name }}</h4>\n            <p>{{ item.announcement_content }}</p>\n          </el-card>\n        </el-timeline-item>\n      </el-timeline>\n    </div>\n    <div class=\"like-btn\">\n      <form id=\"like-it-form\" action=\"#\" method=\"post\">\n        <span class=\"like-it\">66</span>\n        <input type=\"hidden\" name=\"post_id\" value=\"99\" />\n        <input type=\"hidden\" name=\"action\" value=\"like_it\" />\n      </form>\n    </div>\n    <comment />\n  </div>\n</template>\n\n<script>\nimport comment from '@/components/comment.vue'\nimport { mapState, mapActions } from 'vuex'\n\nexport default {\n  components: {\n    comment\n  },\n  computed: {\n    ...mapState({\n      contentuserid: state => state.contentuserid,\n      commentnum: state => state.commentnum\n    })\n  },\n  data() {\n    return {\n      content: {},\n      announcementlist: {}\n    }\n  },\n  props: {\n    id: {\n      type: String,\n      required: true\n    }\n  },\n  methods: {\n    ...mapActions(['setcontentid', 'setcontentinfo']),\n    jubao(username) {\n      let url = escape(window.location.href)\n      this.$router.push({\n        path: '/jubao',\n        query: { user: username, url: url }\n      })\n    },\n    async getactivitycontent() {\n      let data = { id: this.id }\n      let res = await this.$axios.post('/web/getactivitycontent', this.qs.stringify(data))\n      if (res.data.state.type === 'SUCCESS') {\n        this.content = res.data.data\n        this.setcontentinfo({\n          contentname: res.data.data.activity_title,\n          contentuserid: res.data.data.user_id\n        })\n      }\n    },\n    async setjoin() {\n      let data = {\n        type: this.$route.name,\n        contentname: this.content.activity_title,\n        to_userid: this.contentuserid,\n        content_id: this.content.activity_id\n      }\n      let res = await this.$axios.post('/web/setjoin', this.qs.stringify(data))\n      if (res.data.state.type === 'SUCCESS') {\n        this.$message.success('参加成功')\n      } else {\n        this.$message.error('你已经报名参加了本项目')\n      }\n    },\n    async getannouncementlist() {\n      let res = await this.$axios.post('/admin/announcementlist', this.qs.stringify({ content_id: this.id }))\n      if (res.data.state.type === 'SUCCESS') {\n        this.announcementlist = res.data.data\n      }\n    }\n  },\n  watch: {\n    id: {\n      handler(newVal) {\n        this.getactivitycontent()\n        this.getannouncementlist()\n        this.setcontentid(this.id)\n      },\n      deep: true,\n      immediate: true\n    }\n  },\n  created() {\n    this.getannouncementlist()\n    this.setcontentid(this.id)\n  }\n}\n</script>\n\n<style scoped>\n.activitycontent {\n  min-height: 200px;\n  padding-left: 20px; /* 增加左边距 */\n}\n.tagname {\n  margin-right: 16px;\n  font-size: 18px;\n}\n.show_unit {\n  margin-bottom: 25px; /* 增加行间隔 */\n}\n</style>\n"],"sourceRoot":"src/components"}]}